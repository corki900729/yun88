.page-contents-wrapper .lay02 .lay02-inner .product-list .y-wrap {
  display: inline-block;
  width: 180px;
  height: 180px;
  background: url(../../images/index/bg2.png);
  background-position: center center;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -o-border-radius: 50%;
}
.page-contents-wrapper .lay02 .lay02-inner .product-list .y-wrap:hover {
  animation: move 2s linear 0.2s infinite alternate;
  -webkit-animation: move 2s linear 0.2s infinite alternate;
  -moz-animation: move 2s linear 0.2s infinite alternate;
  -o-animation: move 2s linear 0.2s infinite alternate;
}
@keyframes move {
  0% {
    transform: scale(1);
  }
  25% {
    transform: scale(1.1);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(1.1);
  }
}
@-webkit-keyframes move {
  0% {
    transform: scale(1);
  }
  25% {
    transform: scale(1.1);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(1.1);
  }
}
@-moz-keyframes move {
  0% {
    transform: scale(1);
  }
  25% {
    transform: scale(1.1);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(1.1);
  }
}
@-o-keyframes move {
  0% {
    transform: scale(1);
  }
  25% {
    transform: scale(1.1);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(1.1);
  }
}
