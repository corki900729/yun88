/**
* Common classes.
* mainly for tweeking or other minor styles.
*/
* {
  box-sizing: border-box;
}
body {
  overflow: auto;
}
.form-block > * {
  display: inline-block;
  vertical-align: middle;
}
h1 {
  font-size: 23px;
}
h2 {
  font-size: 18px;
}
h3 {
  font-size: 16px;
}
h4 {
  font-size: 14px;
}
span {
  font-size: 14px;
}
p {
  font-size: 14px;
}
.page-contents-wrapper {
  overflow: hidden;
}
.floor {
  padding: 2em 0;
}
.floor.f-one {
  padding-top: 4em;
}
.floor.f-btm {
  padding-bottom: 0;
}
body {
  width: 100%;
  *zoom: 1;
}
body:after,
body:before {
  content: "";
  display: table;
}
body:after {
  clear: both;
}
body:before,
body:after {
  content: "";
  display: table;
}
body:after {
  clear: both;
}
.row {
  *zoom: 1;
  margin-left: -11px;
  margin-right: -11px;
}
.row:after,
.row:before {
  content: "";
  display: table;
}
.row:after {
  clear: both;
}
