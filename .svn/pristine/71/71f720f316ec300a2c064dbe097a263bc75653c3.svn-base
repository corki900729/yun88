  width: 100%;
  height: 498px!important;
  background-position: center center;
  background-size: cover;
}
.page-contents-wrapper .banner .img-holder .banner-inner {
  position: relative;
  height: 498px;
}
.page-contents-wrapper .banner .img-holder .banner-inner .banner-title {
  font-size: 35px;
  color: #d3d3d2;
  position: absolute;
  left: 0;
  top: 72px;
}
.page-contents-wrapper .banner .img-holder .banner-inner .banner-title .zx {
  vertical-align: middle;
  margin-right: 10px;
}
.page-contents-wrapper .banner .img-holder .banner-inner .banner-text1 {
  width: 45%;
  font-size: 16px;
  color: #dcdcdc;
  position: absolute;
  left: 0;
  top: 170px;
  line-height: 25px;
}
.page-contents-wrapper .banner .img-holder .banner-inner .consult2 {
  position: absolute;
  left: 0;
  top: 350px;
}
.page-contents-wrapper .banner .img-holder .banner-inner .consult2 a {
  display: inline-block;
  padding: 15px 45px;
  background: #0668d7;
  color: white;
  font-size: 16px;
  border-radius: 5px;
}
.page-contents-wrapper .lay01 {
  padding: 50px 0;
}
.page-contents-wrapper .lay01 .lay01-title {
  font-size: 25px;
  font-weight: 500;
  border-left: 4px solid #0668d7;
  text-indent: 30px;
}
.page-contents-wrapper .lay01 .list-box .list-block {
  height: 320px;
  margin-top: 20px;
  border-bottom: 2px solid #cccccc;
  position: relative;
}
.page-contents-wrapper .lay01 .list-box .list-block .show-block {
  overflow: hidden;
  height: 320px;
  position: absolute;
  left: 0;
  top: 0;
}
.page-contents-wrapper .lay01 .list-box .list-block .show-block .img-holder {
  text-align: center;
  height: 150px;
  padding-top: 50px;
  vertical-align: bottom;
}
.page-contents-wrapper .lay01 .list-box .list-block .show-block .text-holder {
  overflow: hidden;
}
.page-contents-wrapper .lay01 .list-box .list-block .show-block .text-holder .pro-title {
  text-align: center;
  font-size: 18px;
  font-weight: bold;
  line-height: 40px;
}
.page-contents-wrapper .lay01 .list-box .list-block .show-block .text-holder .pro-text {
  padding: 0 30px;
  overflow: hidden;
  line-height: 25px;
  font-size: 14px;
}
.page-contents-wrapper .lay01 .list-box .list-block .hide-block {
  width: 100%;
  height: 320px;
  position: absolute;
  left: 0;
  top: 0;
  background: #00a0e9;
  color: #ffffff;
}
.page-contents-wrapper .lay01 .list-box .list-block .hide-block .hide-title {
  font-size: 18px;
  text-align: center;
  line-height: 70px;
}
.page-contents-wrapper .lay01 .list-box .list-block .hide-block .hide-text {
  font-size: 14px;
  line-height: 30px;
